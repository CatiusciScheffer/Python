function AplicarMascaras(){AplicarMascara(".cpfcnpj",["999.999.999-99","99.999.999/9999-99"]);AplicarMascara(".cpf","999.999.999-99");AplicarMascara(".cnpj","99.999.999/9999-99");AplicarMascara(".telefone",["(99)9999-9999","(99)99999-9999"]);AplicarMascara(".cep","99.999-999");AplicarMascara(".mes-ano","99/9999");AplicarMascara(".data","99/99/9999");AplicarMascara(".numerico","999999999999");AplicarMascara(".num7pos","9999999");AplicarMascara(".num15pos","999999999999999");AplicarMascara(".num30pos","999999999999999999999999999999");AplicarMascara(".reciboIRPF","99.99.99.99.99");AplicarMascara(".chaveNFe","99999999999999999999999999999999999999999999");AplicarMascara(".chaveNFSe","99999999999999999999999999999999999999999999999999")}function AplicarMascara(n,t,i){if(typeof n=="undefined"||n==""||typeof t=="undefined"||t=="")return!1;var r=t;if($.isArray(t)&&t.length>2)return!1;$(n).each(function(){if($.isArray(t)&&(r=NormalizaValor($(this).val()).length<=NormalizaValor(t[0]).length?t[0]:t[1]),$(this).is("input, textarea")){if(VMasker($(this)[0]).unMask(),VMasker($(this)[0]).maskPattern(r),$(this).off("blur.limparValorCampoMascarado"),typeof i!="undefined"&&i)$(this).on("blur.limparValorCampoMascarado",function(){$(this).val().length!=r.length&&$(this).val("")});$(this).off("blur.acionarKeyupNaPerdaDeFoco");$(this).on("blur.acionarKeyupNaPerdaDeFoco",function(){$(this).keyup()});if($.isArray(t))$(this).on("keyup",function(){var i=$(this).val().replace(/\D/g,""),n=0;NormalizaValor($(this).val()).length>NormalizaValor(t[n]).length&&(n=1);VMasker($(this)).unMask();VMasker($(this)).maskPattern(t[n]);$(this).val(VMasker.toPattern(i,t[n]))})}else $(this).text(VMasker.toPattern($(this).text(),r))})}function NormalizaValor(n){var t=n.replace(/\-/g,"");return t=t.replace(/\./g,""),t=t.replace(/\,/g,""),t=t.replace(/\//g,""),t=t.replace(/\(/g,""),t.replace(/\)/g,"")}function FuncoesBasicas(){AplicarMascaras();$('[data-toggle="tooltip"]').tooltip({container:"body"});$('[data-to